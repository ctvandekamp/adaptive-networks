issn = {03036812},
journal = {Journal of Mathematical Biology},
keywords = {Moment closure approximation,Network model,Pair approximation,Sexually transmitted diseases,Steady/casual partnership network},
number = {5},
pages = {375--395},
pmid = {12424529},
title = {{A versatile ODE approximation to a network model for the spread of sexually transmitted diseases}},
volume = {45},
year = {2002}
}
